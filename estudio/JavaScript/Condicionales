var num1 =  30;
var num2 = 40;

if(num1 > num2){

	document.write("Si que es mas grande");

}else if(num1 > 10){

	document.write("Es mas que 10");

}else{

	document.write("Es muy pequeÃ±o");
}

/*
Operadores Logicos

> Mayor que
< Menor que
|| Or
&& AND
== igual a/que (sin tener en cuenta el tipo de dato de la variable)
=== igual a/que (teniendo en cuenta el tipo de dato de cada variable)
!= distindo de
!== distinto de (Lo mismo que el ===)
! No 

*/